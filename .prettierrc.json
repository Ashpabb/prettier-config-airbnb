{
  "$schema": "http://json.schemastore.org/prettierrc",
  "quoteProps": "as-needed",
  "singleQuote": true,
  "arrowParens": "always",
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 100,
  "proseWrap": "always",
  "bracketSpacing": false,
  "trailingComma": "es5",
  "semi": true,

  "bracketSameLine": false,
  "jsxSingleQuote": false
}
